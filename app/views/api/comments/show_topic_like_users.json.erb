<% topic_users = {} %>
<% @topic_votes.each_with_index do |vote,index| %>
    <%
      options = comment_users_hash(vote)
      topic_users.merge!({index => options})
    %>
<% end %>
<%= {
  :status    => 'ok',
  :action    => 'ShowTopicLikeUsers',
  :topic_users => topic_users
}.to_json.html_safe %>
